  "version": "${version}",

  "name": "Cmd Client",
  "description": "CmdClient is a client based on Wurst that utilizes the command system only.",
  "authors": [
    "BOB225225, (Original: Wurst)"
  ],
  "contact": {
    "homepage": "https://www.andrewbruhmoment.github.io/",
    "sources": "Comming Soon"
  },

